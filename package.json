{
  "name": "buzz-word",
  "version": "1.0.0",
  "description": "\"# Buzz-word-back-end\"",
  "main": "app.js",
  "browserslist": "> .5% or last 2 versions",
  "scripts": {
    "start": "node build/app.js",
    "git": "cross-env git checkout development && git add . && git commit -m",
    "git:merge": "cross-env git checkout main && git merge development && git push && git checkout development",
    "clean": "rm build -rf /s /q && mkdir build",
    "compile": "babel src -d build",
    "build": "npm run clean && npm run compile"
  },
