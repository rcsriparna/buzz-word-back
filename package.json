    "url": "https://github.com/madalinacristea/Buzz-word-back-end/issues"
  },
  "homepage": "https://github.com/madalinacristea/Buzz-word-back-end#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "connect-mongo": "^4.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongoose": "^5.12.14",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
